<h2>New Article</h2>
<%= form_for @article, :html => {:multipart => true} do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %><br/>
  <%= f.label :category %><br/>
  <%= f.text_field :category %><br/>
  <%= f.label :text %>
  <%= f.text_area :text %><br/>
  <%= f.label :image %>
  <%= f.file_field :image %><br/>
  <%= f.submit :submit %>
<% end %>
